;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.insec. hostmaster.insec. (
			      1		; Serial
				 60		; Refresh
				 60		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.insec.
@	IN	NS	ns2.insec.
ns1.insec.	IN	A	10.0.0.10
ns2.insec.	IN	A	10.0.0.11
@	IN	PTR	ns2.insec.
